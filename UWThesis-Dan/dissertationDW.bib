	shorttitle   = {Constrained model predictive control},
	author       = {Mayne, {D}. {Q}. and {Rawlings}, {J}. {B}. and {Rao}, {C}. {V}. and {Scokaert}, {P}. {O}. {M}.},
	year         = 2000,
