@inproceedings{Oshima_XChen_DSCC2013_standord,
	title        = {Control Design for Cancellation of Unnatural Reaction Torque and Vibrations in Variable-gear-ratio Steering System},
	author       = {Oshima, Atsushi and Chen, Xu and Sumio Sugita and Tomizuka, Masayoshi},
	year         = 2013,
	month        = oct,
	booktitle    = {Proceedings of 2013 {ASME} Dynamic Systems and Control Conference},
