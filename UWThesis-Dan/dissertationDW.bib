	author       = {Oboe, R. and Beghi, A. and Capretta, P. and Soldavini, F.C.},
	year         = 2002,
	journal      = {IEEE/ASME Transactions on Mechatronics},
	publisher    = {IEEE},
	volume       = 7,
	number       = 2,
