	mendeley-tags = {actuators,computer-aided design,control design environment,control system CAD,digital simulation,disc drives,dual-stage actuators,dual-stage hard disk drives,dynamic coupling,finite wordlength,friction,hard discs,inertia plus resonance model,integrated tool,nonlinear friction modeling,nonrepetitive run outs,position error signal,primary actuator,repetitive run outs,secondary actuator,servocontrollers,servomechanisms,simulation environment,single-stage actuators,single-stage hard disk drives,tracking performance,voice coil motor}
